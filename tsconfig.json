{
  "extends": "./node_modules/fx-style/tsconfig-google.json",
  "compilerOptions": {
    "module": "CommonJS",
    "target": "ES2020",
    "outDir": ".",
    "noImplicitAny": false,
    "removeComments": true,
    "preserveConstEnums": true,
    "sourceMap": false,
    "isolatedModules": false,
    "resolveJsonModule": false,
    "noEmit": false,
    "declaration": false,
    "strict": false,
    "strictNullChecks": false,
    "forceConsistentCasingInFileNames": true,
    "allowJs": true,
    "skipDefaultLibCheck": true,
    "incremental": true,
    "skipLibCheck": true,
    "paths": {
      "types/*": [
        "./@types/*"
      ],
      "utils/*": [
        "./utils/*"
      ]
    },
    "baseUrl": "."
  },
  "include": [
    "./index.ts",
    "./test.ts"
  ],
  "exclude": [
    "node_modules"
  ]
}
