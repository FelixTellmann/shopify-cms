{%- if input_linklist -%}
  {% unless input_linklist.handle contains "-legacy-" %}
    {
    "handle": {{ input_linklist.handle | json }},
    "levels": {{ input_linklist.levels | json }},
    "links": {%- include '_map_links', input_links: input_linklist.links -%},
    "title": {{ input_linklist.title | json }}
    }
  {% else %}
    null
  {% endunless %}
{%- else -%}
  null
{%- endif -%}

